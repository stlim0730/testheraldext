{
  "manifest_version": 2,
 "web_accessible_resources": ["optimisely-sniffer.js"],
  "name": "Optimisely Detector",
  "description": "Optimisely Extension",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "<all_urls>"
      ], 
      "all_frames": false, 
      "run_at": "document_start"
    }
  ],
  "browser_action":{
    "default_popup": "popup.html"
  }
}